    "prisma-client-lib": "^1.21.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "firebase": "^5.5.9",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
